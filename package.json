{
  "name": "graphiql-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run tailwind\" \"vite\"",
    "build": "tsc && vite build",
    "husky-install": "husky install",
    "lint": "eslint ./src --ext .ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "tailwind": "npx tailwindcss -i ./src/index.css -o public/output.css --watch"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": "eslint --cache",
    "src/**/*.{ts,tsx,css,md}": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.5.1",
    "@svgx/vite-plugin-react": "^1.0.1",
    "@types/react-transition-group": "^4.4.6",
    "concurrently": "^8.0.1",
    "firebase": "^9.21.0",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-firebase-hooks": "^5.1.1",
    "react-hook-form": "^7.43.9",
    "react-intl": "^6.4.2",
    "react-router": "^6.11.0",
    "react-router-dom": "^6.11.0",
    "react-transition-group": "^4.4.5"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.3.2",
    "tsc-files": "^1.1.3",
    "typescript": "^5.0.2",
    "vite": "^4.3.2"
  }
}
